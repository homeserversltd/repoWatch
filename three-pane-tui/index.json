{
  "metadata": {
    "schema_version": "1.0.0",
    "name": "Three Pane TUI Display",
    "description": "Three-pane terminal UI displaying git-submodules, committed-not-pushed analysis, and system info"
  },
  "paths": {},
  "children": [
    "core",
    "styles",
    "data",
    "ui",
    "main"
  ],
  "execution": {
    "mode": "standalone",
    "continue_on_error": true,
    "parallel": false
  },
  "config": {
    "title": "repoWatch Three Pane Display",
    "exit_keys": "qQ",
    "pane1_title": "Dirty Files",
    "pane2_title": "Committed Not Pushed",
    "pane3_title": "Live Feed of Changes"
  }
}
