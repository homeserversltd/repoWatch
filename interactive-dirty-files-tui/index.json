{
  "metadata": {
    "schema_version": "1.0.0",
    "name": "Interactive Dirty Files TUI Display",
    "description": "Interactive terminal UI for displaying dirty files with tree/flat view toggle"
  },
  "paths": {
    "report_file": "../dirty-files-report.json"
  },
  "children": [],
  "execution": {
    "mode": "standalone",
    "continue_on_error": true,
    "parallel": false
  },
  "config": {
    "title": "Interactive Dirty Files Analysis",
    "exit_keys": "qQ",
    "refresh_keys": "rR",
    "toggle_keys": " ",
    "refresh_interval": 5000,
    "max_display_files": 50,
    "default_view": "flat",
    "tree_prefix": "├── ",
    "tree_last_prefix": "└── ",
    "tree_indent": "│   "
  }
}
